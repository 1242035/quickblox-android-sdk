<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.quickblox.customobjects">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="19"/>

    <application android:icon="@drawable/app_icon" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.quickblox.customobjects.activities.SplashActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/notes" android:name="com.quickblox.customobjects.activities.DisplayNoteListActivity" android:screenOrientation="portrait"/>
        <activity android:label="@string/add_new_note" android:name="com.quickblox.customobjects.activities.AddNewNoteActivity" android:screenOrientation="portrait"/>
        <activity android:label="@string/note_title" android:name="com.quickblox.customobjects.activities.ShowNoteActivity" android:screenOrientation="portrait"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
