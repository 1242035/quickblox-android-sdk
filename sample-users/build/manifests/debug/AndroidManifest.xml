<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.quickblox.sample.user">

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="19"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="QuickBlox User Sample" android:name="com.quickblox.sample.user.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="Users" android:name="com.quickblox.sample.user.activities.GetAllUsersActivity"/>
        <activity android:label="Sign Up" android:name="com.quickblox.sample.user.activities.SignUpUserActivity"/>
